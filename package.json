{
  "name": "nextjs-typescript-starter",
  "private": true,
  "version": "0.1.0",
  "description": "Next.js TypeScript Starter",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "eslint:check": "eslint --ext .ts,.tsx --format friendly ./",
    "eslint:fix": "eslint --fix --ext .ts,.tsx --format friendly ./",
    "stylelint:check": "stylelint styles/**/*.scss",
    "stylelint:fix": "stylelint styles/**/*.scss --fix",
    "analyze": "cross-env ANALYZE=true next build",
    "analyze:server": "cross-env BUNDLE_ANALYZE=server next build",
    "analyze:browser": "cross-env BUNDLE_ANALYZE=browser next build",
    "prepare": "husky install"
  },
  "dependencies": {
    "next": "11.1.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "sass": "^1.43.2",
    "swr": "^1.0.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.2.1",
    "@commitlint/config-conventional": "^13.2.0",
    "@next/bundle-analyzer": "^11.1.2",
    "@types/node": "^16.11.1",
    "@types/react": "17.0.31",
    "@types/react-dom": "^17.0.10",
    "@typescript-eslint/eslint-plugin": "^5.1.0",
    "commitizen": "^4.2.4",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.32.0",
    "eslint-config-next": "11.1.2",
    "eslint-config-prettier": "^8.3.0",
    "eslint-formatter-friendly": "^7.0.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.26.1",
    "husky": "^7.0.4",
    "lint-staged": "^11.2.3",
    "prettier": "^2.4.1",
    "stylelint": "^13.13.1",
    "stylelint-config-standard": "^22.0.0",
    "typescript": "4.4.4"
  }
}

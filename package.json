{
  "name": "nextjs-typescript-starter",
  "private": true,
  "version": "1.1.0",
  "description": "Next.js TypeScript Starter",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "export": "next export",
    "start": "next start",
    "eslint:check": "eslint --ext .ts,.tsx --format friendly ./",
    "eslint:fix": "eslint --fix --ext .ts,.tsx --format friendly ./",
    "stylelint:check": "stylelint src/styles/**/*.scss",
    "stylelint:fix": "stylelint src/styles/**/*.scss --fix",
    "analyze": "cross-env ANALYZE=true next build",
    "analyze:server": "cross-env BUNDLE_ANALYZE=server next build",
    "analyze:browser": "cross-env BUNDLE_ANALYZE=browser next build",
    "prepare": "husky install"
  },
  "dependencies": {
    "next": "^13.1.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sass": "^1.43.3",
    "swr": "^2.0.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.2",
    "@commitlint/config-conventional": "^17.4.2",
    "@next/bundle-analyzer": "^13.1.6",
    "@types/node": "^18.11.18",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@typescript-eslint/eslint-plugin": "^5.1.0",
    "commitizen": "^4.2.4",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.33.0",
    "eslint-config-next": "^13.1.6",
    "eslint-config-prettier": "^8.3.0",
    "eslint-formatter-friendly": "^7.0.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.0",
    "postcss-scss": "^4.0.2",
    "prettier": "^2.4.1",
    "stylelint": "^15.3.0",
    "stylelint-config-standard": "^31.0.0",
    "typescript": "^5.0.2"
  }
}

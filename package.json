{
  "name": "nextjs-typescript-starter",
  "private": true,
  "version": "1.3.0",
  "description": "Next.js TypeScript Starter",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "export": "next export",
    "start": "next start",
    "eslint:check": "eslint --ext .ts,.tsx --format friendly ./",
    "eslint:fix": "eslint --fix --ext .ts,.tsx --format friendly ./",
    "stylelint:check": "stylelint src/styles/**/*.scss",
    "stylelint:fix": "stylelint src/styles/**/*.scss --fix",
    "analyze": "cross-env ANALYZE=true next build",
    "analyze:server": "cross-env BUNDLE_ANALYZE=server next build",
    "analyze:browser": "cross-env BUNDLE_ANALYZE=browser next build",
    "prepare": "husky install"
  },
  "dependencies": {
    "next": "13.5.6",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "sass": "1.75.0"
  },
  "devDependencies": {
    "@commitlint/cli": "17.8.1",
    "@commitlint/config-conventional": "17.8.1",
    "@next/bundle-analyzer": "13.5.6",
    "@types/node": "18.19.31",
    "@types/react": "18.3.0",
    "@types/react-dom": "18.3.0",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "commitizen": "4.3.0",
    "cross-env": "7.0.3",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "8.57.0",
    "eslint-config-next": "13.5.6",
    "eslint-config-prettier": "8.10.0",
    "eslint-formatter-friendly": "7.0.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.34.1",
    "eslint-plugin-simple-import-sort": "10.0.0",
    "eslint-plugin-unused-imports": "2.0.0",
    "husky": "8.0.3",
    "lint-staged": "13.3.0",
    "postcss": "8.4.38",
    "postcss-scss": "4.0.9",
    "prettier": "2.8.8",
    "stylelint": "15.11.0",
    "stylelint-config-standard": "32.0.0",
    "typescript": "5.1.6"
  }
}
